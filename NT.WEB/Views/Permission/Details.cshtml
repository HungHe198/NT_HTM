@model NT.SHARED.Models.Permission

<h2>Chi tiết quyền</h2>
<dl class="row">
    <dt class="col-sm-2">Mã quyền</dt>
    <dd class="col-sm-10">@Model.Code</dd>
    <dt class="col-sm-2">Mô tả</dt>
    <dd class="col-sm-10">@Model.Description</dd>
</dl>
<p>
    <a asp-action="Edit" asp-route-id="@Model.Id" class="btn btn-primary">Sửa</a> |
    <a asp-action="Index" class="btn btn-secondary">Quay lại danh sách</a>
</p>

